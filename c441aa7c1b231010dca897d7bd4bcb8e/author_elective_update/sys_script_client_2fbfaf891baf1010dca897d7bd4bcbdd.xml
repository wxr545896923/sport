        <name>Calculate per capita consumption</name>
        <order/>
        <script><![CDATA[function onChange() {
   //Type appropriate comment here, and begin script below
   var changdi = g_form.getValue('place_money');
	var qita = g_form.getValue('other_money');
	var renshu = g_form.getValue('perosn_num');
	var renjun = (parseFloat(changdi) + parseFloat(qita))/renshu;
	alert("人均消费：" + renjun +"元。");
	//清除原有表单数据
			g_form.clearValue('money_of_person');
			//在表单中设定新数据
			g_form.setValue('money_of_person',renjun);
	}]]></script>
